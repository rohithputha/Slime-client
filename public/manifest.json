{
  "short_name": "React App",
  "name": "Create React App Sample",

  "display": "standalone",
  "theme_color": "#000000",
  "background_color": "#ffffff",



  "manifest_version": 3,
  "version": "1.0",
  "permissions": [
    "contextMenus",
    "activeTab",
    "scripting",
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "index.html"
  }
}
